  };

  camldo1_reg: camldo1 {
  };

  camldo2_reg: camldo2 {
  };

  simldo1_reg: simldo1 {
  };

  simldo2_reg: simldo2 {
  };

  sdldo_reg: sdldo {
  };

  sdxldo_reg: sdxldo {
  };

  mmcldo1_reg: mmcldo1 {
  };

  mmcldo2_reg: mmcldo2 {
  };

  audldo_reg: audldo {
  };

  micldo_reg: micldo {
  };

  usbldo_reg: usbldo {
  };

  vibldo_reg: vibldo {
  };

  csr_reg: csr {
  };

  iosr1_reg: iosr1 {
  };

  iosr2_reg: iosr2 {
