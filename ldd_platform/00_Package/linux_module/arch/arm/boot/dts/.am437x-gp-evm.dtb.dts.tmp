# 1 "arch/arm/boot/dts/am437x-l4.dtsi" 1
&l4_wkup {
 compatible = "ti,am4-l4-wkup", "simple-pm-bus";
 power-domains = <&prm_wkup>;
 clocks = <&l4_wkup_clkctrl ((0x220) - 0x220) 0>;
 clock-names = "fck";
