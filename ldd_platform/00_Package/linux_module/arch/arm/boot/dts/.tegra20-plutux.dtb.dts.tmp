  interrupts = <0 45 4>;
  clocks = <&tegra_car 99>,
    <&tegra_car 98>;
  clock-names = "out", "in";
  resets = <&tegra_car 10>;
  dmas = <&apbdma 3>, <&apbdma 3>;
  dma-names = "rx", "tx";
  #sound-dai-cells = <0>;
  status = "disabled";

  assigned-clocks = <&tegra_car 99>;
  assigned-clock-parents = <&tegra_car 113>;
 };

 tegra_i2s1: i2s@70002800 {
  compatible = "nvidia,tegra20-i2s";
  reg = <0x70002800 0x200>;
  interrupts = <0 13 4>;
  clocks = <&tegra_car 11>;
  resets = <&tegra_car 11>;
  reset-names = "i2s";
  dmas = <&apbdma 2>, <&apbdma 2>;
  dma-names = "rx", "tx";
