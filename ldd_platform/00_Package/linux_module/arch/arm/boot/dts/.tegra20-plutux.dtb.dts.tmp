   reg = <1>;
   #address-cells = <1>;
   #size-cells = <0>;
  };
 };

 i2c@7000d000 {
  clock-frequency = <400000>;
  status = "okay";

  pmic: tps6586x@34 {
   compatible = "ti,tps6586x";
   reg = <0x34>;
   interrupts = <0 86 4>;

   ti,system-power-controller;

   #gpio-cells = <2>;
   gpio-controller;


   sys-supply = <&vdd_5v0_reg>;
   vin-sm0-supply = <&sys_reg>;
