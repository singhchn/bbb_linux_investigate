 das@70000c00 {
  compatible = "nvidia,tegra20-das";
  reg = <0x70000c00 0x80>;
 };

 tegra_ac97: ac97@70002000 {
  compatible = "nvidia,tegra20-ac97";
  reg = <0x70002000 0x200>;
  interrupts = <0 81 4>;
  clocks = <&tegra_car 3>;
  resets = <&tegra_car 3>;
  reset-names = "ac97";
  dmas = <&apbdma 12>, <&apbdma 12>;
  dma-names = "rx", "tx";
