};

&sdmmc0 {
 bus-width = <8>;
 non-removable;
 no-1-8-v;
 sdhci-caps-mask = <0x0 0x00200000>;
 pinctrl-names = "default";
 pinctrl-0 = <&pinctrl_sdmmc0_default>;
 status = "okay";
};

&sdmmc1 {
