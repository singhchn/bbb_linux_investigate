     regulator-allowed-modes = <2>, <4>;
     regulator-always-on;

     regulator-state-standby {
      regulator-on-in-suspend;
      regulator-suspend-microvolt = <1350000>;
      regulator-mode = <4>;
     };

     regulator-state-mem {
      regulator-on-in-suspend;
      regulator-suspend-microvolt = <1350000>;
      regulator-mode = <4>;
