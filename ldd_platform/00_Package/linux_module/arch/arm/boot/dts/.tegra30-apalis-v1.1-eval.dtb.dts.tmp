   interrupts = <0 73 4>;
   clocks = <&tegra_car 27>,
     <&tegra_car 179>;
   clock-names = "dc", "parent";
   resets = <&tegra_car 27>;
   reset-names = "dc";
   power-domains = <&pd_core>;
   operating-points-v2 = <&disp1_dvfs_opp_table>;

