  power-domains = <&pd_heg>;
  operating-points-v2 = <&host1x_dvfs_opp_table>;

  #address-cells = <1>;
  #size-cells = <1>;

  ranges = <0x54000000 0x54000000 0x04000000>;

  mpe@54040000 {
