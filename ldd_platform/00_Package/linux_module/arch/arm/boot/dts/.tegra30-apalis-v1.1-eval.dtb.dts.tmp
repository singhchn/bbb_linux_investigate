  dc@54240000 {
   compatible = "nvidia,tegra30-dc";
   reg = <0x54240000 0x00040000>;
   interrupts = <0 74 4>;
   clocks = <&tegra_car 26>,
     <&tegra_car 179>;
   clock-names = "dc", "parent";
   resets = <&tegra_car 26>;
   reset-names = "dc";
   power-domains = <&pd_core>;
   operating-points-v2 = <&disp2_dvfs_opp_table>;

   iommus = <&mc 2>;

   nvidia,head = <1>;

   interconnects = <&mc 2 &emc>,
