   compatible = "samsung,exynos4210-combiner";
   #interrupt-cells = <2>;
   interrupt-controller;
   samsung,combiner-nr = <32>;
   reg = <0x10440000 0x1000>;
   interrupts = <0 0 4>,
         <0 1 4>,
         <0 2 4>,
         <0 3 4>,
         <0 4 4>,
         <0 5 4>,
         <0 6 4>,
         <0 7 4>,
         <0 8 4>,
         <0 9 4>,
         <0 10 4>,
         <0 11 4>,
         <0 12 4>,
