 };

 clocks {
  sys_clk: sys_clk {
   compatible = "fixed-clock";
   #clock-cells = <0>;
   clock-frequency = <165625000>;
  };

  cpu_clk: cpu_clk {
   compatible = "fixed-clock";
   #clock-cells = <0>;
   clock-frequency = <600000000>;
