    };

    port6: port@6 {
     reg = <6>;
     status = "disabled";
    };

    port7: port@7 {
     reg = <7>;
     status = "disabled";
    };
   };
  };

  flx0: flexcom@e0040000 {
   compatible = "atmel,sama5d2-flexcom";
   reg = <0xe0040000 0x100>;
   clocks = <&clks 7>;
   #address-cells = <1>;
   #size-cells = <1>;
