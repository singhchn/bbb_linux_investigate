   interrupt-names = "xtr", "fdma", "ana", "ptp",
       "ptp-ext";
   resets = <&reset 0>;
   reset-names = "switch";
   status = "disabled";

   ethernet-ports {
    #address-cells = <1>;
    #size-cells = <0>;

    port0: port@0 {
     reg = <0>;
     status = "disabled";
