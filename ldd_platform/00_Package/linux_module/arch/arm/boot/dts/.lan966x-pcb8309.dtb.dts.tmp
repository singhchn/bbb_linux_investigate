  udc: usb@200000 {
   compatible = "microchip,lan9662-udc",
         "atmel,sama5d3-udc";
   reg = <0x00200000 0x80000>,
         <0xe0808000 0x400>;
   interrupts = <0 76 4>;
   clocks = <&clks 15>, <&nic_clk>;
   clock-names = "pclk", "hclk";
   status = "disabled";
