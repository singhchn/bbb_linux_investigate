     status = "disabled";
    };

    port2: port@2 {
     reg = <2>;
     status = "disabled";
    };

    port3: port@3 {
     reg = <3>;
     status = "disabled";
    };

    port4: port@4 {
     reg = <4>;
     status = "disabled";
    };

    port5: port@5 {
