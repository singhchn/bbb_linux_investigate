# 1 "arch/arm/boot/dts/vt8500-bv07.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm/boot/dts/vt8500-bv07.dts"







/dts-v1/;
/include/ "vt8500.dtsi"

/ {
 model = "Benign BV07 Netbook";
};

&fb {
 bits-per-pixel = <16>;
 display-timings {
  native-mode = <&timing0>;
  timing0: 800x480 {
   clock-frequency = <0>;
   hactive = <800>;
   vactive = <480>;
   hfront-porch = <40>;
   hback-porch = <88>;
   hsync-len = <0>;
   vback-porch = <32>;
   vfront-porch = <11>;
   vsync-len = <1>;
  };
 };
};

&uart0 {
 status = "okay";
};
