&i2c1 {
 led-controller@38 {
  compatible = "ti,lm3532";
  #address-cells = <1>;
  #size-cells = <0>;
  reg = <0x38>;

  enable-gpios = <&gpio6 12 0>;

  ramp-up-us = <1024>;
  ramp-down-us = <8193>;

  backlight_led: led@0 {
   reg = <0>;
   led-sources = <2>;
   ti,led-mode = <0>;
   label = ":backlight";
  };

  led@1 {
   reg = <1>;
   led-sources = <1>;
   ti,led-mode = <0>;
   label = ":kbd_backlight";
  };
 };
};

&i2c4 {
 lis3dh: accelerometer@18 {
  compatible = "st,lis3dh-accel";
  reg = <0x18>;

  vdd-supply = <&vhvio>;

  interrupt-parent = <&gpio2>;
  interrupts = <2 (2 | 1)>;

  rotation-matrix = "0", "-1", "0",
      "1", "0", "0",
      "0", "0", "1";
 };
};
