    clocks = <&l3_div_ck>;
    clock-names = "fck";
    interrupt-controller;
    #interrupt-cells = <2>;
    gpio-controller;
    #gpio-cells = <2>;
   };
  };

  target-module@52000000 {
   compatible = "ti,sysc-omap4", "ti,sysc";
   reg = <0x52000000 0x4>,
         <0x52000010 0x4>;
