/ {
 compatible = "samsung,p4note", "samsung,exynos4412", "samsung,exynos4";

 memory@40000000 {
  device_type = "memory";
  reg = <0x40000000 0x80000000>;
 };

 chosen {
  stdout-path = &serial_2;
 };

 firmware@204f000 {
  compatible = "samsung,secure-firmware";
  reg = <0x0204F000 0x1000>;
 };

 fixed-rate-clocks {
  xxti {
   compatible = "samsung,clock-xxti";
   clock-frequency = <0>;
  };

  xusbxti {
   compatible = "samsung,clock-xusbxti";
   clock-frequency = <24000000>;
  };
 };

 gpio-keys {
  compatible = "gpio-keys";
  pinctrl-names = "default";
  pinctrl-0 = <&gpio_keys>;

  key-down {
   gpios = <&gpx2 2 1>;
   linux,code = <114>;
   label = "volume down";
   debounce-interval = <10>;
  };

  key-up {
   gpios = <&gpx3 3 1>;
   linux,code = <115>;
   label = "volume up";
   debounce-interval = <10>;
  };

  key-power {
   gpios = <&gpx2 7 1>;
   linux,code = <116>;
   label = "power";
   debounce-interval = <10>;
   wakeup-source;
  };
 };

 voltage-regulator-1 {
  compatible = "regulator-fixed";
  regulator-name = "TSP_LDO1";
  pinctrl-names = "default";
  pinctrl-0 = <&tsp_reg_gpio_1>;
  gpios = <&gpm4 5 0>;
  enable-active-high;
  regulator-always-on;
 };

 voltage-regulator-2 {
  compatible = "regulator-fixed";
  regulator-name = "TSP_LDO2";
  pinctrl-names = "default";
  pinctrl-0 = <&tsp_reg_gpio_2>;
  gpios = <&gpb 5 0>;
  enable-active-high;
  regulator-always-on;
