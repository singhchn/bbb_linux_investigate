     reg = <0x400 0x200>;
     interrupts = <19 4 7>;
     #address-cells = <1>;
     #size-cells = <0>;
     clocks = <&pmc 2 19>;
     clock-names = "spi_clk";
     dmas = <&dma0
      ((((0) & (0x1)) << (13)) |
       (((1) & (0x1)) << (14)) |
       (((11) & (0x7f)) << (24)))>,
            <&dma0
      ((((0) & (0x1)) << (13)) |
       (((1) & (0x1)) << (14)) |
       (((12) & (0x7f)) << (24)))>;
     dma-names = "tx", "rx";
     atmel,fifo-size = <16>;
     status = "disabled";
    };

    i2c2: i2c@600 {
     compatible = "atmel,sama5d2-i2c";
     reg = <0x600 0x200>;
     interrupts = <19 4 7>;
     #address-cells = <1>;
     #size-cells = <0>;
     clocks = <&pmc 2 19>;
     dmas = <&dma0
      ((((0) & (0x1)) << (13)) |
       (((1) & (0x1)) << (14)) |
       (((11) & (0x7f)) << (24)))>,
            <&dma0
      ((((0) & (0x1)) << (13)) |
       (((1) & (0x1)) << (14)) |
       (((12) & (0x7f)) << (24)))>;
     dma-names = "tx", "rx";
     atmel,fifo-size = <16>;
     status = "disabled";
    };
   };

   flx1: flexcom@f8038000 {
    compatible = "atmel,sama5d2-flexcom";
    reg = <0xf8038000 0x200>;
    clocks = <&pmc 2 20>;
    #address-cells = <1>;
    #size-cells = <1>;
