   resets = <&reset 0>;
   reset-names = "switch";
   gpio-controller;
   #gpio-cells = <2>;
   gpio-ranges = <&gpio 0 0 78>;
   interrupt-controller;
   interrupts = <0 17 4>;
   #interrupt-cells = <2>;
  };

