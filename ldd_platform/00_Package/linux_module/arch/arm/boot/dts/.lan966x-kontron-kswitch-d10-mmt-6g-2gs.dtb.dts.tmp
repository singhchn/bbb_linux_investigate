# 8 "arch/arm/boot/dts/lan966x-kontron-kswitch-d10-mmt.dtsi" 2
# 1 "./scripts/dtc/include-prefixes/dt-bindings/phy/phy-lan966x-serdes.h" 1
# 9 "arch/arm/boot/dts/lan966x-kontron-kswitch-d10-mmt.dtsi" 2

/ {
 aliases {
  serial0 = &usart0;
 };

 chosen {
  stdout-path = "serial0:115200n8";
 };

 gpio-restart {
  compatible = "gpio-restart";
  pinctrl-0 = <&reset_pins>;
  pinctrl-names = "default";
  gpios = <&gpio 56 1>;
  priority = <200>;
 };
};

&flx0 {
 atmel,flexcom-mode = <1>;
 status = "okay";

 usart0: serial@200 {
  pinctrl-0 = <&usart0_pins>;
  pinctrl-names = "default";
  status = "okay";
 };
};

&flx3 {
 atmel,flexcom-mode = <2>;
 status = "okay";

 spi3: spi@400 {
  pinctrl-0 = <&fc3_b_pins>, <&spi3_cs_pins>;
  pinctrl-names = "default";
  status = "okay";
  cs-gpios = <&gpio 46 1>;
 };
};

&gpio {
 fc3_b_pins: fc3-b-pins {

  pins = "GPIO_51", "GPIO_52", "GPIO_53";
  function = "fc3_b";
 };

 miim_c_pins: miim-c-pins {

  pins = "GPIO_59", "GPIO_60";
  function = "miim_c";
 };

 reset_pins: reset-pins {

  pins = "GPIO_56";
  function = "gpio";
 };

 sgpio_a_pins: sgpio-a-pins {

  pins = "GPIO_32", "GPIO_33", "GPIO_34";
  function = "sgpio_a";
 };

 sgpio_b_pins: sgpio-b-pins {

  pins = "GPIO_64";
  function = "sgpio_b";
 };

 spi3_cs_pins: spi3-cs-pins {

  pins = "GPIO_46";
  function = "gpio";
 };

 usart0_pins: usart0-pins {

  pins = "GPIO_25", "GPIO_26";
  function = "fc0_b";
 };

 usbs_a_pins: usbs-a-pins {

  pins = "GPIO_66";
  function = "gpio";
 };
};

&mdio0 {
 pinctrl-0 = <&miim_c_pins>;
 pinctrl-names = "default";
 reset-gpios = <&gpio 29 1>;
 clock-frequency = <2500000>;
 status = "okay";

 phy4: ethernet-phy@5 {
  reg = <5>;
  coma-mode-gpios = <&gpio 37 (2 | 4)>;
 };

 phy5: ethernet-phy@6 {
  reg = <6>;
  coma-mode-gpios = <&gpio 37 (2 | 4)>;
 };

 phy6: ethernet-phy@7 {
  reg = <7>;
  coma-mode-gpios = <&gpio 37 (2 | 4)>;
 };

 phy7: ethernet-phy@8 {
  reg = <8>;
  coma-mode-gpios = <&gpio 37 (2 | 4)>;
 };
};

&mdio1 {
 status = "okay";
};

&phy0 {
 status = "okay";
};

&phy1 {
 status = "okay";
};

&port0 {
 phys = <&serdes 0 (0)>;
 phy-handle = <&phy0>;
 phy-mode = "gmii";
 status = "okay";
};

&port1 {
 phys = <&serdes 1 (1)>;
 phy-handle = <&phy1>;
 phy-mode = "gmii";
 status = "okay";
};

&port4 {
 phys = <&serdes 4 ((2) + 1 + (2))>;
