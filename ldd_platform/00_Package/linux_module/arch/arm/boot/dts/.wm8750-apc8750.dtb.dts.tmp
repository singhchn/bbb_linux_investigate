# 1 "arch/arm/boot/dts/wm8750-apc8750.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm/boot/dts/wm8750-apc8750.dts"
# 9 "arch/arm/boot/dts/wm8750-apc8750.dts"
/dts-v1/;
/include/ "wm8750.dtsi"

/ {
 model = "VIA APC8750";
};

&pinctrl {
 pinctrl-names = "default";
 pinctrl-0 = <&i2c>;

 i2c: i2c {
  wm,pins = <168 169 170 171>;
  wm,function = <2>;
  wm,pull = <2>;
 };
};

&uart0 {
 status = "okay";
};
