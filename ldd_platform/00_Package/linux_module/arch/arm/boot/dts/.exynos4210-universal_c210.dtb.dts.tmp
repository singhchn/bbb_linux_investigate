  spi_2: spi@13940000 {
   compatible = "samsung,exynos4210-spi";
   reg = <0x13940000 0x100>;
   interrupts = <0 68 4>;
   dmas = <&pdma0 9>, <&pdma0 8>;
   dma-names = "tx", "rx";
   #address-cells = <1>;
   #size-cells = <0>;
   clocks = <&clock 329>, <&clock 161>;
   clock-names = "spi", "spi_busclk0";
   pinctrl-names = "default";
   pinctrl-0 = <&spi2_bus>;
   status = "disabled";
