   };
  };

  cpu0: cpu@900 {
   device_type = "cpu";
   compatible = "arm,cortex-a9";
   reg = <0x900>;
   clocks = <&clock 12>;
   clock-names = "cpu";
   clock-latency = <160000>;

   operating-points = <
    1200000 1250000
    1000000 1150000
    800000 1075000
    500000 975000
    400000 975000
    200000 950000
   >;
   #cooling-cells = <2>;
  };

  cpu1: cpu@901 {
   device_type = "cpu";
   compatible = "arm,cortex-a9";
   reg = <0x901>;
   clocks = <&clock 12>;
   clock-names = "cpu";
   clock-latency = <160000>;

   operating-points = <
    1200000 1250000
    1000000 1150000
    800000 1075000
    500000 975000
    400000 975000
    200000 950000
   >;
   #cooling-cells = <2>;
