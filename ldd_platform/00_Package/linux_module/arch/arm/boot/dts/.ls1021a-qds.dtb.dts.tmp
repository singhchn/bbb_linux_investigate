  };

  pwm7: pwm@2a40000 {
   compatible = "fsl,vf610-ftm-pwm";
   #pwm-cells = <3>;
   reg = <0x0 0x2a40000 0x0 0x10000>;
   clock-names = "ftm_sys", "ftm_ext",
    "ftm_fix", "ftm_cnt_clk_en";
   clocks = <&clockgen 4 1>, <&clockgen 4 1>,
