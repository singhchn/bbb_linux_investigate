   status = "disabled";
  };

  wdog0: watchdog@2ad0000 {
   compatible = "fsl,imx21-wdt";
   reg = <0x0 0x2ad0000 0x0 0x10000>;
   interrupts = <0 115 4>;
   clocks = <&clockgen 4 1>;
   clock-names = "wdog-en";
   big-endian;
  };

  sai1: sai@2b50000 {
   #sound-dai-cells = <0>;
   compatible = "fsl,vf610-sai";
   reg = <0x0 0x2b50000 0x0 0x10000>;
   interrupts = <0 132 4>;
   clocks = <&clockgen 4 1>, <&clockgen 4 1>,
