     temperature = <85000>;
     hysteresis = <2000>;
     type = "passive";
    };
    cpu_crit: cpu-crit {
     temperature = <95000>;
     hysteresis = <2000>;
     type = "critical";
    };
   };

   cooling-maps {
    map0 {
     trip = <&cpu_alert>;
     cooling-device =
      <&cpu0 (~0)
      (~0)>,
      <&cpu1 (~0)
      (~0)>;
    };
   };
  };
