   status = "disabled";
  };

  pwm6: pwm@2a30000 {
   compatible = "fsl,vf610-ftm-pwm";
   #pwm-cells = <3>;
   reg = <0x0 0x2a30000 0x0 0x10000>;
   clock-names = "ftm_sys", "ftm_ext",
