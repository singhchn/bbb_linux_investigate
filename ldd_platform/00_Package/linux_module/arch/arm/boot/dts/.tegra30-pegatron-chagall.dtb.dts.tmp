   resets = <&tegra_car 60>;
   reset-names = "mpe";
   power-domains = <&pd_mpe>;
   operating-points-v2 = <&mpe_dvfs_opp_table>;

   iommus = <&mc 5>;

   status = "disabled";
  };

  vi@54080000 {
