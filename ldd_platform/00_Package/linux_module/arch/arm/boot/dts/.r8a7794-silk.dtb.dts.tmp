  cpu1: cpu@1 {
   device_type = "cpu";
   compatible = "arm,cortex-a7";
   reg = <1>;
   clock-frequency = <1000000000>;
   clocks = <&cpg 0 0>;
   power-domains = <&sysc 6>;
   enable-method = "renesas,apmu";
   next-level-cache = <&L2_CA7>;
  };

  L2_CA7: cache-controller-0 {
   compatible = "cache";
   power-domains = <&sysc 21>;
   cache-unified;
   cache-level = <2>;
  };
 };


