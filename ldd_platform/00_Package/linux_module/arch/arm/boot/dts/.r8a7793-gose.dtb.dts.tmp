/ {
 model = "Gose";
 compatible = "renesas,gose", "renesas,r8a7793";

 aliases {
  serial0 = &scif0;
  serial1 = &scif1;
  i2c9 = &gpioi2c2;
  i2c10 = &gpioi2c4;
  i2c11 = &i2chdmi;
  i2c12 = &i2cexio4;
  mmc0 = &sdhi0;
  mmc1 = &sdhi1;
  mmc2 = &sdhi2;
 };

 chosen {
  bootargs = "ignore_loglevel rw root=/dev/nfs ip=on";
  stdout-path = "serial0:115200n8";
 };

 memory@40000000 {
