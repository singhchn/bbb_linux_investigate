/ {
 #address-cells = <1>;
 #size-cells = <1>;
 model = "Qualcomm MDM9615";
 compatible = "qcom,mdm9615";
 interrupt-parent = <&intc>;

 cpus {
  #address-cells = <1>;
  #size-cells = <0>;

  cpu0: cpu@0 {
   compatible = "arm,cortex-a5";
   device_type = "cpu";
