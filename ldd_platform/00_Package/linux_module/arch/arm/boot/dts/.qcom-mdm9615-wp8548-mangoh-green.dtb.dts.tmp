# 254 "arch/arm/boot/dts/qcom-mdm9615-wp8548-mangoh-green.dts"
    #gpio-cells = <2>;
    #interrupt-cells = <2>;
    compatible = "semtech,sx1509q";
    reg = <0x70>;
    interrupt-parent = <&gpioext1>;
    interrupts = <14 2>;

    probe-reset;

    gpio-controller;
    interrupt-controller;
   };
  };

  i2c@7 {
   #address-cells = <1>;
   #size-cells = <0>;
   reg = <7>;
  };
 };
};

&sdcc1 {
 pinctrl-0 = <&sdc_cd_pins>;
 pinctrl-names = "default";
 disable-wp;
 cd-gpios = <&msmgpio 42 1>;
};
