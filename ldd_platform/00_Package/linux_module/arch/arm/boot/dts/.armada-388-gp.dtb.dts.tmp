   };

   usb3_1: usb3@f8000 {
    compatible = "marvell,armada-380-xhci";
    reg = <0xf8000 0x4000>,<0xfc000 0x4000>;
    interrupts = <0 17 4>;
    clocks = <&gateclk 10>;
    status = "disabled";
