    interrupts = <0 75 4>;
    clocks = <&gateclk 0>;
    clock-names = "internal";
    status = "disabled";
   };

   usb3_0: usb3@f0000 {
    compatible = "marvell,armada-380-xhci";
    reg = <0xf0000 0x4000>,<0xf4000 0x4000>;
    interrupts = <0 16 4>;
    clocks = <&gateclk 9>;
