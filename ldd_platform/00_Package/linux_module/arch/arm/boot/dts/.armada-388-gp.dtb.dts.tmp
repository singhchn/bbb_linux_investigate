  crypto_sram0: sa-sram0 {
   compatible = "mmio-sram";
   reg = <(((0x09) << 24) | ((0x19) << 16)) 0 0x800>;
   clocks = <&gateclk 23>;
   #address-cells = <1>;
   #size-cells = <1>;
   ranges = <0 (((0x09) << 24) | ((0x19) << 16)) 0 0x800>;
