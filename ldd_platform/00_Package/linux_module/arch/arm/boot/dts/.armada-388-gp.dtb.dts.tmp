    ge1_rgmii_pins: ge-rgmii-pins-1 {
     marvell,pins = "mpp21", "mpp27", "mpp28",
             "mpp29", "mpp30", "mpp31",
             "mpp32", "mpp37", "mpp38",
             "mpp39", "mpp40", "mpp41";
