  crypto_sram1: sa-sram1 {
   compatible = "mmio-sram";
   reg = <(((0x09) << 24) | ((0x15) << 16)) 0 0x800>;
   clocks = <&gateclk 21>;
   #address-cells = <1>;
   #size-cells = <1>;
   ranges = <0 (((0x09) << 24) | ((0x15) << 16)) 0 0x800>;
  };

  bm_bppi: bm-bppi {
   compatible = "mmio-sram";
   reg = <(((0x0c) << 24) | ((0x04) << 16)) 0 0x100000>;
   ranges = <0 (((0x0c) << 24) | ((0x04) << 16)) 0 0x100000>;
   #address-cells = <1>;
   #size-cells = <1>;
   clocks = <&gateclk 13>;
