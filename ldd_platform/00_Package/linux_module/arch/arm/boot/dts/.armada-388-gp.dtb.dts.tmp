   nand_controller: nand-controller@d0000 {
    compatible = "marvell,armada370-nand-controller";
    reg = <0xd0000 0x54>;
    #address-cells = <1>;
    #size-cells = <0>;
    interrupts = <0 84 4>;
    clocks = <&coredivclk 0>;
    status = "disabled";
   };

   sdhci: sdhci@d8000 {
    compatible = "marvell,armada-380-sdhci";
    reg-names = "sdhci", "mbus", "conf-sdio3";
    reg = <0xd8000 0x1000>,
     <0xdc000 0x100>,
     <0x18454 0x4>;
    interrupts = <0 25 4>;
