   rtc: rtc@a3800 {
    compatible = "marvell,armada-380-rtc";
    reg = <0xa3800 0x20>, <0x184a0 0x0c>;
    reg-names = "rtc", "rtc-soc";
    interrupts = <0 21 4>;
   };

   ahci0: sata@a8000 {
    compatible = "marvell,armada-380-ahci";
    reg = <0xa8000 0x2000>;
    interrupts = <0 26 4>;
    clocks = <&gateclk 15>;
