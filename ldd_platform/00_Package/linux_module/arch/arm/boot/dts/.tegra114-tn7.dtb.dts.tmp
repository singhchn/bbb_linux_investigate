  dmas = <&apbdma 1>, <&apbdma 1>,
         <&apbdma 2>, <&apbdma 2>,
         <&apbdma 3>, <&apbdma 3>,
         <&apbdma 4>, <&apbdma 4>,
         <&apbdma 6>, <&apbdma 6>,
         <&apbdma 7>, <&apbdma 7>,
         <&apbdma 12>, <&apbdma 12>,
