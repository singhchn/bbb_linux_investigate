  dma-names = "rx0", "tx0", "rx1", "tx1", "rx2", "tx2",
       "rx3", "tx3", "rx4", "tx4", "rx5", "tx5",
       "rx6", "tx6", "rx7", "tx7", "rx8", "tx8",
       "rx9", "tx9";
  ranges;
  #address-cells = <1>;
  #size-cells = <1>;

  tegra_i2s0: i2s@70080300 {
   compatible = "nvidia,tegra114-i2s", "nvidia,tegra30-i2s";
   reg = <0x70080300 0x100>;
   nvidia,ahub-cif-ids = <4 4>;
   clocks = <&tegra_car 30>;
   resets = <&tegra_car 30>;
   reset-names = "i2s";
   status = "disabled";
