  clock-names = "d_audio", "apbif";
  resets = <&tegra_car 106>,
    <&tegra_car 107>,
    <&tegra_car 30>,
    <&tegra_car 11>,
    <&tegra_car 18>,
    <&tegra_car 101>,
    <&tegra_car 102>,
    <&tegra_car 108>,
    <&tegra_car 109>,
    <&tegra_car 110>,
    <&tegra_car 10>,
    <&tegra_car 153>,
    <&tegra_car 154>;
  reset-names = "d_audio", "apbif", "i2s0", "i2s1", "i2s2",
