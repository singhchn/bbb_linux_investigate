 interrupt-parent = <&gic>;
 #address-cells = <1>;
 #size-cells = <1>;

 cpus {
  #address-cells = <1>;
  #size-cells = <0>;
  cpu@0 {
   compatible = "arm,cortex-a9";
   device_type = "cpu";
   reg = <0x0>;
   clock-frequency = <800000000>;
   power-domains = <&pd_a3sm>;
   next-level-cache = <&L2>;
  };
 };

 gic: interrupt-controller@c2800000 {
  compatible = "arm,pl390";
  #interrupt-cells = <3>;
  interrupt-controller;
  reg = <0xc2800000 0x1000>,
        <0xc2000000 0x1000>;
 };

