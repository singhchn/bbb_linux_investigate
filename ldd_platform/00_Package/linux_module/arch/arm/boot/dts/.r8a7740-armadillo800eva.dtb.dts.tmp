  compatible = "arm,pl310-cache";
  reg = <0xf0100000 0x1000>;
  interrupts = <0 84 4>;
  power-domains = <&pd_a3sm>;
  arm,data-latency = <3 3 3>;
  arm,tag-latency = <2 2 2>;
  arm,shared-override;
  cache-unified;
  cache-level = <2>;
 };

